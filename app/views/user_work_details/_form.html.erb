<%= simple_form_for(@user_work_detail) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :login %>
    <%= f.input :logout %>
    <%= f.input :total_time %>
    <%= f.association :user %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
